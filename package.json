    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "prettier": "3.8.1",
    "prettier-plugin-astro": "0.14.1",
    "typescript-eslint": "8.56.0",
    "unocss": "^66.6.0",
    "vue-eslint-parser": "10.4.0"
